}



<!doctype html>
<html lang="es">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

    <title>Masterplan Runa Residencial</title>
    <style>
        .disponible {
            fill: #ffc740a8;
            stroke: transparent;
        }

            .disponible:hover {
                fill: #ffc740b3;
            }

        .no-disponible {
            fill: #4a0e81;
            cursor: not-allowed;
            stroke: #4a0e81;
        }

        .reservado {
            fill: #0e19a3;
            cursor: not-allowed;
            stroke: #00000b;
        }

        .mp-header {
            background-color: white;
            height: 50px;
            text-align: center;
            padding: 20px;
        }

        .cls-2, .cls-1 {
            fill: white;
            pointer-events: none;
        }

        .modal-content {
            /*background-image: url('https://gaiahabitat.com/img/leaf-bg.png');
            background-size: 17%;
            background-repeat: no-repeat;
            background-position: top right;*/
            border-radius: 24px;
            border-color:  #b4903a;
            border-width: 6px;
        }

        .modal-header {
            border-bottom: 0px solid white;
        }

        .modal-body strong {
            color:  #b4903a;
        }

        #Capa_1 {
            border-radius: 2px;
        }
    </style>
</head>
<body>
    <div class="container-fluid">
        <div class="row masterplan">
            <div class="col-12 mp-body p-0" style="background: #141325;">
                <svg id="Capa_1" data-name="Capa 1" xmlns="https://www.w3.org/2000/svg" xmlns:xlink="https://www.w3.org/1999/xlink" viewBox="0 0 1143 643">
                    <image width="1143" height="643" xlink:href="../Content/masterplanbg.jpg" />

                    @foreach (var item in terrainList)
                    {
                        if (item.isSold)
                        {
                            <polygon id="lote-@(item.terrainId)" 
                            dataname="lote-@(item.terrainId)" class="no-disponible" points="@item.svgPoint" />
                        }
                        else
                        {
                            if (item.isPending)
                            {
                                <polygon id="lote-@(item.terrainId)" 
                                dataname="lote-@(item.terrainId)" class="reservado" points="@item.svgPoint" />
                            }
                            else
                            {
                                <polygon id="lote-@(item.terrainId)"
                                dataname="lote-@(item.terrainId)" class="disponible" points="@item.svgPoint" href="#" data-toggle="modal" data-target="#Modal-id-@item.terrainId" />
                            }
                        }
                    }


