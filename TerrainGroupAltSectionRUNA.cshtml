@{
    Layout = null;
}
@using ReservationService.Model;
@using ReservationService;

@{
    MasterPlanGroup masterPlanGroup = (MasterPlanGroup)ViewBag.masterPlanGroup;
    List<Terrain> terrainList = (List<Terrain>)ViewBag.terrainList;
    int sectionId = (int)ViewBag.sectionId;

    String lang = Request.UserLanguages[0];
